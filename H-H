#include <bits/stdc++.h>
using namespace std;

vector<long long int> v;
long long int i, a, cnt; 
int main()
{
    long long int n, m;
    while (cin >> n >> m && n != 0 && m != 0)
    {
        cnt = 0;
        for (i = 0; i < n; i++)
        {
            cin >> a;
            v.push_back(a);
        }

        for (i = 0; i < m; i++)
        {
            cin >> a;

            for (auto it = v.begin(); it != v.end(); it++)
            {
                if (*it == a)
                {
                    *it = 0;
                    cnt++;
                    break;
                }
            }
        }

        cout << cnt << endl;
    }
}
